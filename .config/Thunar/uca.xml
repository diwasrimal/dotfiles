<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1581593230771688-1</unique-id>
	<command>/usr/bin/xterm</command>
	<description>Example for a custom action</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>go-parent-folder</icon>
	<name>Open as root</name>
	<submenu></submenu>
	<unique-id>1622790717174452-1</unique-id>
	<command>~/.config/openbox/rofi/bin/apps_as_root &apos;dbus-run-session thunar&apos;</command>
	<description>Open directory as root</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>document-edit-sign</icon>
	<name>Edit as root</name>
	<submenu></submenu>
	<unique-id>1622790919788057-2</unique-id>
	<command>~/.config/openbox/rofi/bin/apps_as_root &apos;geany %f&apos;</command>
	<description>Edit file as root</description>
	<range></range>
	<patterns>*</patterns>
	<text-files/>
</action>
<action>
	<icon>kr_comparedirs</icon>
	<name>Compare</name>
	<submenu></submenu>
	<unique-id>1622791692322694-4</unique-id>
	<command>meld %F</command>
	<description>Compare files and directories with  meld</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<text-files/>
</action>
<action>
	<icon>kr_diskusage</icon>
	<name>Disk Usage</name>
	<submenu></submenu>
	<unique-id>1622791401529558-3</unique-id>
	<command>baobab %d</command>
	<description>Disk usages by directory and its sub directories</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>view-preview</icon>
	<name>Set as wallpaper</name>
	<submenu></submenu>
	<unique-id>1622798756568291-1</unique-id>
	<command>nitrogen --save --set-zoom-fill %f</command>
	<description>Set wallpaper with nitrogen in openbox</description>
	<range></range>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
	<icon>rotation-locked-landscape</icon>
	<name>Set as lockscreen</name>
	<submenu></submenu>
	<unique-id>1622799434407260-2</unique-id>
	<command>notify-send &apos;Please wait...&apos; &amp;&amp; betterlockscreen -u %f</command>
	<description>Set image as lockscreen background</description>
	<range></range>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
	<icon>application-pdf</icon>
	<name>Generate PDF</name>
	<submenu></submenu>
	<unique-id>1680619086361010-1</unique-id>
	<command>dmenu -p &quot;ÔáÅ PDF:&quot; &lt;/dev/null | xargs -I{} convert %F {}</command>
	<description>Generate PDF from image files</description>
	<range>*</range>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
	<icon>application-pdf</icon>
	<name>Generate PDF</name>
	<submenu></submenu>
	<unique-id>1681047712832275-1</unique-id>
	<command>soffice --headless --convert-to pdf %F &gt;/dev/null 2&gt;&amp;1 || notify-send &quot;Error!&quot; &quot;Make sure you have libreoffice&quot;</command>
	<description>Generate PDFs using libreoffice command line utility</description>
	<range>*</range>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
</actions>
