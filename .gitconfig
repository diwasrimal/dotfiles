[alias]
    b = branch
    d = diff
    ds = diff --staged
    au = add -u
    ap = add -p
    cl = clone --depth 1
    cm = commit -m 
    lg = log --oneline --graph
    st = status -sb
    sw = switch
    lsf = ls-files
    lst = ls-tree -r --name-only
    rmc = rm --cached
    res = restore --staged
    undo = reset --soft HEAD~1
    amend = commit --amend
    recon = rebase --continue
[user]
    name = Diwas Rimal
    email = rimaldiwas10@gmail.com
[color "status"]
    changed = blue
[init]
    defaultBranch = main
